{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "schematics": {
    "@schematics/angular:component": {
      "changeDetection": "OnPush",
      "style": "scss"
    }
  },
  "projects": {
    "epgu-form-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/epgu-form-frontend/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "./projects/epgu-constructor/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "./projects/epgu-constructor-ui-kit/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@epgu/ui/assets/",
                "output": "./lib-assets"
              },
              {
                "glob": "**/*",
                "input": "./projects/children-clubs/src/assets",
                "output": "assets"
              }
            ],
            "styles": ["src/styles.scss"],
            "scripts": ["vendors/luna-pass-0.0.1a.js"]
          },
          "configurations": {
            "fast": {
              "tsConfig": "./tsconfig.app.fast.json",
              "vendorChunk": true,
              "extractLicenses": false,
              "buildOptimizer": false,
              "sourceMap": true,
              "optimization": false,
              "namedChunks": true
            },
            "development": {
              "tsConfig": "./tsconfig.app.dev.json",
              "vendorChunk": true,
              "extractLicenses": false,
              "buildOptimizer": false,
              "sourceMap": true,
              "optimization": false,
              "namedChunks": true
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "aot": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "tsConfig": "./tsconfig.app.prod.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "defaultConfiguration": "",
          "options": {
            "host": "local.test.gosuslugi.ru",
            "browserTarget": "epgu-form-frontend:build:development",
            "port": 4200,
            "proxyConfig": "src/proxy.conf.json"
          },
          "configurations": {
            "fast": {
              "browserTarget": "epgu-form-frontend:build:fast"
            },
            "development": {
              "browserTarget": "epgu-form-frontend:build:development"
            },
            "production": {
              "browserTarget": "epgu-form-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "epgu-form-frontend:build"
          }
        }
      }
    },
    "epgu-constructor": {
      "projectType": "library",
      "root": "projects/epgu-constructor",
      "sourceRoot": "projects/epgu-constructor/src",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "prefix": "epgu-constructor",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/epgu-constructor/tsconfig.lib.json",
            "project": "projects/epgu-constructor/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/epgu-constructor/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "projects/epgu-constructor/.eslintrc.js",
            "lintFilePatterns": [
              "projects/epgu-constructor/src/**/*.ts",
              "projects/epgu-constructor/src/**/*.spec.ts"
            ]
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run"
        }
      }
    },
    "children-clubs": {
      "projectType": "library",
      "root": "projects/children-clubs",
      "sourceRoot": "projects/children-clubs/src",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "prefix": "children-clubs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/children-clubs/tsconfig.lib.json",
            "project": "projects/children-clubs/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/children-clubs/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "projects/children-clubs/.eslintrc.js",
            "lintFilePatterns": [
              "projects/children-clubs/src/**/*.ts",
              "projects/children-clubs/src/**/*.spec.ts"
            ]
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run"
        }
      }
    },
    "epgu-constructor-ui-kit": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/epgu-constructor-ui-kit",
      "sourceRoot": "projects/epgu-constructor-ui-kit/src",
      "prefix": "epgu-cf-ui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/epgu-constructor-ui-kit/tsconfig.lib.json",
            "project": "projects/epgu-constructor-ui-kit/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/epgu-constructor-ui-kit/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "projects/epgu-constructor-ui-kit/.eslintrc.js",
            "lintFilePatterns": [
              "projects/epgu-constructor-ui-kit/src/**/*.ts",
              "projects/epgu-constructor-ui-kit/src/**/*.spec.ts"
            ]
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run"
        }
      }
    },
    "epgu-constructor-docs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/epgu-constructor-docs",
      "sourceRoot": "projects/epgu-constructor-docs/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/epgu-constructor-docs",
            "index": "projects/epgu-constructor-docs/src/index.html",
            "main": "projects/epgu-constructor-docs/src/main.ts",
            "polyfills": "projects/epgu-constructor-docs/src/polyfills.ts",
            "tsConfig": "projects/epgu-constructor-docs/tsconfig.app.json",
            "assets": [
              "projects/epgu-constructor-docs/src/favicon.ico",
              "projects/epgu-constructor-docs/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@taiga-ui/icons/src",
                "output": "./assets/taiga-ui/icons"
              },
              {
                "glob": "**/*",
                "input": "./projects/epgu-constructor-ui-kit/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@epgu/ui/assets/",
                "output": "./lib-assets"
              }
            ],
            "styles": [
              "projects/epgu-constructor-docs/src/styles.scss",
              "node_modules/@taiga-ui/core/styles/taiga-ui-theme.less",
              "node_modules/@taiga-ui/core/styles/taiga-ui-global.less"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/epgu-constructor-docs/src/environments/environment.ts",
                  "with": "projects/epgu-constructor-docs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "epgu-constructor-docs:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "epgu-constructor-docs:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "epgu-constructor-docs:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "projects/epgu-constructor-docs/.eslintrc.js",
            "lintFilePatterns": [
              "projects/epgu-constructor-docs/src/**/*.ts",
              "projects/epgu-constructor-docs/src/**/*.spec.ts"
            ]
          }
        }
      }
    },
    "sf-portal": {
      "projectType": "application",
      "root": "projects/sf-portal",
      "sourceRoot": "projects/sf-portal/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sf-portal",
            "index": "projects/sf-portal/src/index.html",
            "main": "projects/sf-portal/src/main.ts",
            "polyfills": "projects/sf-portal/src/polyfills.ts",
            "tsConfig": "projects/sf-portal/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "preserveSymlinks": true,
            "assets": [
              {
                "glob": "**/*",
                "input": "./node_modules/@epgu/ui/assets",
                "output": "./lib-assets"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@epgu/epgu-constructor/src/assets",
                "output": "./assets"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@epgu/epgu-constructor-ui-kit/src/assets",
                "output": "./assets"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@epgu/children-clubs/src/assets",
                "output": "./assets"
              },
              "projects/sf-portal/src/favicon.ico",
              "projects/sf-portal/src/assets"
            ],
            "styles": ["projects/sf-portal/src/styles.scss"],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/sf-portal/src/environments/environment.ts",
                  "with": "projects/sf-portal/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sf-portal:build:production"
            },
            "development": {
              "browserTarget": "sf-portal:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sf-portal:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run"
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/sf-portal/server",
            "main": "projects/sf-portal/server.ts",
            "tsConfig": "projects/sf-portal/tsconfig.server.json"
          },
          "configurations": {
            "fast": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            },
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "projects/sf-portal/src/environments/environment.ts",
                  "with": "projects/sf-portal/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "sf-portal:build:development",
              "serverTarget": "sf-portal:server:development"
            },
            "production": {
              "browserTarget": "sf-portal:build:production",
              "serverTarget": "sf-portal:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": ["/"]
          },
          "configurations": {
            "production": {
              "browserTarget": "sf-portal:build:production",
              "serverTarget": "sf-portal:server:production"
            },
            "development": {
              "browserTarget": "sf-portal:build:development",
              "serverTarget": "sf-portal:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "versions": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/versions",
      "sourceRoot": "projects/versions/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/versions",
            "index": "projects/versions/src/index.html",
            "main": "projects/versions/src/main.ts",
            "polyfills": "projects/versions/src/polyfills.ts",
            "tsConfig": "projects/versions/tsconfig.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/versions/src/favicon.ico",
              "projects/versions/src/assets"
            ],
            "styles": [
              "projects/versions/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/versions/src/environments/environment.ts",
                  "with": "projects/versions/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "projects/versions/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "versions:build:production"
            },
            "development": {
              "browserTarget": "versions:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "versions:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "projects/versions/.eslintrc.js",
            "lintFilePatterns": [
              "projects/versions/src/**/*.ts",
              "projects/versions/src/**/*.spec.ts"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "epgu-form-frontend",
  "cli": {
    "analytics": false,
    "defaultCollection": "@datorama/akita"
  }
}
