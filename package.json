{
  "name": "epgu-form-frontend",
  "version": "0.0.0",
  "scripts": {
    "______develop": "echo list of command for developing",
    "ng": "ng",
    "start": "npm run start-proxy",
    "start-stage": "ng serve --configuration staging",
    "start-proxy": "ng serve --proxy-config ./src/proxy.conf.json",
    "start-backend": "java -jar fs-1.0-SNAPSHOT.jar --server.servlet.context-path=/api",
    "______build": "echo list of command for build",
    "build": "ng build",
    "______library": "echo list of command for library",
    "library-build": "ng build epgu-constructor",
    "library-watch": "ng build epgu-constructor --watch",
    "library-test": "ng test epgu-constructor",
    "library-lint": "ng lint epgu-constructor",
    "______checking": "echo list of command for checking",
    "lint": "ng lint",
    "lint: auto-fix": "ng lint --fix",
    "______test": "echo list of command for testing",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@angular/animations": "~10.0.4",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/forms": "~10.0.4",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/router": "~10.0.4",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "angular2-text-mask": "9.0.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "epgu-lib": "^0.0.202",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "moment": "2.24.0",
    "ngx-perfect-scrollbar": "8.0.0",
    "prettier": "^2.0.5",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular-devkit/build-ng-packagr": "~0.1000.5",
    "@angular-eslint/builder": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
    "@angular-eslint/schematics": "0.0.1-alpha.32",
    "@angular-eslint/template-parser": "0.0.1-alpha.32",
    "@angular/cli": "~10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "codelyzer": "^6.0.0",
    "eslint": "^6.8.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~3.9.5"
  }
}
