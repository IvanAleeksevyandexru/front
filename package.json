{
  "name": "@epgu/epgu-form-frontend",
  "version": "42.0.0",
  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main --first-only",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check --hmr --ssl",
    "start:prod": "ng serve --host 0.0.0.0 --disable-host-check --hmr --ssl --configuration production",
    "start:dev": "ng serve --host 0.0.0.0 --disable-host-check --hmr --ssl --configuration development",
    "start:fast": "concurrently -k --restart-tries 10 -n \"CONFIG,START\" \"yarn start:config\" \"ng serve --host 0.0.0.0 --disable-host-check --hmr --ssl --configuration fast\"",
    "start:fast:uat": "concurrently -k --restart-tries 10 -n \"CONFIG,START\" \"yarn start:config:uat\" \"ng serve --host 0.0.0.0 --disable-host-check --hmr --ssl --configuration fast\"",
    "start:stage": "ng serve --configuration staging",
    "start:backend": "java -jar pgu-form-service-1.0-SNAPSHOT.jar --server.servlet.context-path=/api --draft-client.enabled=false",
    "start:backend:local": "java -jar ./fs-1.0-SNAPSHOT.jar --server.servlet.context-path=/api --spring.profiles.active=local",
    "start:config": "json-server --watch form-player-config.json --routes form-player-config-routes.json",
    "start:config:uat": "json-server --watch form-player-config.uat.json --routes form-player-config-routes.json",
    "start:portal": "ng serve sf-portal --host vbeta.test.gosuslugi.ru --port 8888 --ssl",
    "build: portal": "ng build sf-portal --configuration production",
    "test:portal": "ng test sf-portal",
    "lint:portal": "ng lint sf-portal",
    "copy-dev-i18n": "cpx \"dist/sf-portal-st/dev/lib-assets/i18n/**/*\" \"dist/sf-portal-st/dev/lib-assets/i18n\"",
    "copy-dev2-i18n": "cpx \"dist/sf-portal-st/dev2/lib-assets/i18n/**/*\" \"dist/sf-portal-st/dev2/lib-assets/i18n\"",
    "copy-demo-i18n": "cpx \"dist/sf-portal-st/demo/lib-assets/i18n/**/*\" \"dist/sf-portal-st/demo/lib-assets/i18n\"",
    "copy-express-i18n": "cpx \"dist/sf-portal-st/express/lib-assets/i18n/**/*\" \"dist/sf-portal-st/express/lib-assets/i18n\"",
    "copy-rcod-i18n": "cpx \"dist/sf-portal-st/rcod/lib-assets/i18n/**/*\" \"dist/sf-portal-st/rcod/lib-assets/i18n\"",
    "copy-svcdev-i18n": "cpx \"dist/sf-portal-st/svcdev/lib-assets/i18n/**/*\" \"dist/sf-portal-st/svcdev/lib-assets/i18n\"",
    "copy-uat-i18n": "cpx \"dist/sf-portal-st/uat/lib-assets/i18n/**/*\" \"dist/sf-portal-st/uat/lib-assets/i18n\"",
    "copy-prod-i18n": "cpx \"dist/sf-portal-st/prod/lib-assets/i18n/**/*\" \"dist/sf-portal-st/prod/lib-assets/i18n\"",
    "prebuild-all": "npm run update-version",
    "preprod": "npm run update-version",
    "prebuild": "npm run update-version",
    "update-version": "npm --no-git-tag-version version patch",
    "postbuild-all": "npm run copy-all-i18n",
    "copy-all-i18n": "npm run copy-dev-i18n && npm run copy-dev2-i18n && npm run copy-express-i18n && npm run copy-rcod-i18n && npm run copy-svcdev-i18n && npm run copy-demo-i18n && npm run copy-uat-i18n && npm run copy-prod-i18n",
    "build-all": "cross-env rimraf ./dist && ng build --configuration production --deploy-url https://gu-st.ru/sf-portal-st/ && npm run copy-static-all",
    "copy-static-all": "cross-env cpx \"dist/browser/**/*\" dist/sf-portal-st/prod && rimraf ./dist/browser && npm run copy-env-folders && npm run replace",
    "copy-env-folders": "node copy-env-folders",
    "replace": "node replace",
    "build": "ng build --configuration production",
    "lib:type:build": "cd ./projects/epgu-constructor-types && yarn build",
    "lib:type:watch": "cd ./projects/epgu-constructor-types && yarn watch",
    "lib:children-clubs:build": "ng build children-clubs --configuration production",
    "lib:children-clubs:build:dev": "ng build children-clubs",
    "lib:children-clubs:watch": "yarn wait-types && yarn wait-ui-kit && ng build children-clubs --watch",
    "lib:ui-kit:build": "ng build epgu-constructor-ui-kit --configuration production",
    "lib:ui-kit:build:dev": "ng build epgu-constructor-ui-kit",
    "lib:ui-kit:watch": "yarn wait-types && ng build epgu-constructor-ui-kit --watch",
    "lib:docs:build": "ng build epgu-constructor-docs --configuration production",
    "lib:docs:build:dev": "ng build epgu-constructor-docs --watch",
    "lib:docs:watch": "ng serve epgu-constructor-docs",
    "lib:build": "ng build epgu-constructor --configuration production",
    "lib:cf:watch": "yarn wait-types && yarn wait-ui-kit && ng build epgu-constructor --watch",
    "lib:build:ivy": "ng build epgu-constructor",
    "lib:watch": "yarn clean && concurrently -k --restart-tries 10 -n \"TYPE,UI-KIT,CHILDREN-CLUBS,CF\" \"yarn lib:type:watch\" \"yarn lib:ui-kit:watch\" \"yarn lib:children-clubs:watch\" \"yarn lib:cf:watch\"",
    "lib:test": "ng test epgu-constructor",
    "lib:lint": "yarn workspaces run lint",
    "lib:lint:fix": "yarn workspaces run lint:fix",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "stylelint": "stylelint \"**/*.scss\" --config .stylelintrc.json",
    "stylelint:fix": "stylelint \"**/*.scss\" --config .stylelintrc.json --fix",
    "format": "npx prettier --write . && yarn lint:fix",
    "test": "jest --ci",
    "test:dev": "jest --runInBand --watch -o",
    "test:coverage": "jest --ci --coverage --no-cache --reporters=default --reporters=jest-junit --coverageDirectory=coverage/jest",
    "test:dry": "jscpd projects/epgu-constructor --silent --ignore \"**/*.min.js,**/*.map,**/*.spec.ts,**/*.module.ts,**/*.stub.ts\" --format \"javascript,scss,typescript\" --threshold 0",
    "test:dry:dev": "jscpd projects/epgu-constructor --reporters json,html,console --blame --ignore \"**/*.min.js,**/*.map,**/*.spec.ts,**/*.module.ts,**/*.stub.ts\" --format \"javascript,scss,typescript\" --threshold 0",
    "ci": "yarn clean && yarn lint && yarn stylelint && yarn test && yarn lib:type:build && yarn lib:ui-kit:build && yarn lib:children-clubs:build && yarn lib:build && yarn build",
    "release": "standard-version",
    "generate-lib-version-ts": "node scripts/version.js",
    "e2e": "ng e2e",
    "docs:json": "compodoc -p ./tsconfig.base.json -e json -d .",
    "wait-types": "wait-on projects/epgu-constructor-types/dist/index.js",
    "wait-ui-kit": "wait-on dist/epgu-constructor-ui-kit/package.json",
    "wait-children-clubs": "wait-on dist/children-clubs/package.json",
    "wait-cf": "wait-on dist/epgu-constructor/package.json",
    "clean": "rimraf dist && rimraf projects/epgu-constructor-types/dist",
    "watch-no-config:all": "yarn clean && concurrently -k --restart-tries 10 -n \"LIB,START\" \"yarn lib:watch\" \"yarn wait-cf && yarn start\"",
    "watch:all": "yarn clean && concurrently -k --restart-tries 10 -n \"CONFIG,LIB,START\" \"yarn start:config\" \"yarn lib:watch\" \"yarn wait-cf && yarn start\"",
    "build:dev:all": "yarn clean && concurrently -k -n \"CONFIG,BUILD AND START\" \"yarn start:config\" \"yarn lib:type:build && yarn lib:ui-kit:build:dev && yarn lib:children-clubs:build:dev && yarn lib:build:ivy && yarn start\"",
    "build:prod:libs": "yarn clean && concurrently -k -n \"CONFIG,BUILD AND START\" \"yarn start:config\" \"yarn lib:type:build && yarn lib:ui-kit:build && yarn lib:children-clubs:build && yarn lib:build && yarn start:prod\"",
    "build:prod:all": "yarn clean && concurrently -k -n \"CONFIG,BUILD AND START\" \"yarn start:config\" \"yarn lib:type:build && yarn lib:ui-kit:build && yarn lib:children-clubs:build && yarn lib:build && yarn build\""
  },
  "private": true,
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged --relative",
      "pre-push": "yarn ci"
    }
  },
  "jest-junit": {
    "suiteName": "epgu-form-frontend",
    "outputDirectory": "./coverage/junit",
    "outputName": "test-results.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "dependencies": {
    "@alyle/ui": "10.5.1",
    "@angular/animations": "12.2.5",
    "@angular/cdk": "12.2.5",
    "@angular/common": "12.2.5",
    "@angular/compiler": "12.2.5",
    "@angular/core": "12.2.5",
    "@angular/forms": "12.2.5",
    "@angular/platform-browser": "12.2.5",
    "@angular/platform-browser-dynamic": "12.2.5",
    "@angular/router": "12.2.5",
    "@datorama/akita": "6.2.0",
    "@epgu/children-clubs": "42.0.0",
    "@epgu/epgu-constructor-types": "42.0.0",
    "@epgu/epgu-constructor-ui-kit": "42.0.0",
    "@epgu/ui": "3.0.391-1-frontv5",
    "@epgu/zipkin": "0.0.3",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@nicky-lenaers/ngx-scroll-to": "9.0.0",
    "@tanker/file-ponyfill": "^2.6.3",
    "angular-resize-event": "^2.0.0",
    "angular2-text-mask": "9.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.8.1",
    "date-fns": "2.21.3",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "ics": "^2.24.0",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "4.17.21",
    "ngx-cookie-service": "10.0.1",
    "ngx-device-detector": "^2.0.0",
    "ngx-perfect-scrollbar": "8.0.0",
    "ngx-webcam": "0.3.0",
    "node-fetch": "^2.6.1",
    "os": "^0.1.1",
    "postcss": "^8.2.4",
    "ru-validation-codes": "2.8.0",
    "rxjs": "6.6.6",
    "tslib": "2.1.0",
    "uuid": "8.3.0",
    "web-animations-js": "^2.3.2",
    "zipkin-transport-http": "^0.22.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "12.1.2",
    "@angular-devkit/build-angular": "12.2.5",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
    "@angular-eslint/schematics": "0.0.1-alpha.32",
    "@angular-eslint/template-parser": "0.0.1-alpha.32",
    "@angular-extensions/pretty-html-log": "^4.0.1",
    "@angular/cli": "12.2.5",
    "@angular/compiler-cli": "12.2.5",
    "@babel/core": "^7.12.10",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@compodoc/compodoc": "^1.1.11",
    "@datorama/akita-ngdevtools": "^6.0.0",
    "@stryker-mutator/core": "^4.4.1",
    "@stryker-mutator/jest-runner": "^4.4.1",
    "@stryker-mutator/typescript-checker": "^4.4.1",
    "@types/file-saver": "^2.0.1",
    "@types/hammerjs": "^2.0.38",
    "@types/jest": "27.0.2",
    "@types/node": "12.11.1",
    "@types/uuid": "8.3.0",
    "@types/yandex-maps": "^2.1.20",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.2.2",
    "codelyzer": "6.0.0",
    "commitlint-plugin-function-rules": "^1.1.20",
    "concurrently": "^6.2.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "9.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.1.4",
    "http-proxy-middleware": "^2.0.1",
    "husky": "4.2.5",
    "jest": "27.3.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-junit": "12.2.0",
    "jest-preset-angular": "10.1.0",
    "jscpd": "^3.3.23",
    "json-server": "^0.16.2",
    "lint-staged": "10.2.11",
    "ng-mocks": "11.5.0",
    "ng-packagr": "^12.2.1",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "stylelint": "13.6.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "ts-jest": "27.0.7",
    "typescript": "4.3.5",
    "wait-on": "^5.3.0"
  },
  "workspaces": [
    "projects/*"
  ]
}
