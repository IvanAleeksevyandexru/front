{
  "name": "epgu-form-frontend",
  "version": "0.7.0",
  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "start:stage": "ng serve --configuration staging",
    "start:backend": "java -jar pgu-form-service-1.0-SNAPSHOT.jar --server.servlet.context-path=/api --draft-client.enabled=false",
    "start:config": "json-server --watch form-player-config.json --routes form-player-config-routes.json",
    "build": "ng build --configuration production",
    "lib:build": "ng build epgu-constructor --prod",
    "lib:build:ivy": "ng build epgu-constructor",
    "lib:watch": "ng build epgu-constructor --watch",
    "lib:test": "ng test epgu-constructor",
    "lib:lint": "ng lint epgu-constructor",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "stylelint": "stylelint \"**/*.scss\" --config .stylelintrc.json",
    "stylelint:fix": "stylelint \"**/*.scss\" --config .stylelintrc.json --fix",
    "test": "jest --ci",
    "test:dev": "jest --runInBand --watch -o",
    "test:coverage": "jest --ci --coverage --no-cache --reporters=default --reporters=jest-junit --coverageDirectory=coverage/jest",
    "ci": "yarn lint && yarn stylelint && yarn test && yarn lib:build:ivy && yarn lib:build && yarn build",
    "release": "standard-version",
    "generate-lib-version-ts": "node scripts/version.js",
    "e2e": "ng e2e"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative",
      "pre-push": "yarn ci"
    }
  },
  "jest-junit": {
    "suiteName": "epgu-form-frontend",
    "outputDirectory": "./coverage/junit",
    "outputName": "test-results.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "dependencies": {
    "@alyle/ui": "^10.2.1",
    "@angular/animations": "10.1.1",
    "@angular/cdk": "10.2.0",
    "@angular/common": "10.1.1",
    "@angular/compiler": "10.1.1",
    "@angular/core": "10.1.1",
    "@angular/forms": "10.1.1",
    "@angular/platform-browser": "10.1.1",
    "@angular/platform-browser-dynamic": "10.1.1",
    "@angular/router": "10.1.1",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@tanker/file-ponyfill": "^2.6.3",
    "@types/file-saver": "^2.0.1",
    "@types/uuid": "8.3.0",
    "angular-resize-event": "^2.0.0",
    "angular2-text-mask": "9.0.0",
    "core-js": "^3.8.1",
    "epgu-lib": "0.0.755",
    "file-saver": "^2.0.5",
    "ics": "^2.24.0",
    "moment": "2.24.0",
    "ngx-cookie-service": "10.0.1",
    "ngx-device-detector": "^2.0.0",
    "ngx-perfect-scrollbar": "8.0.0",
    "ngx-webcam": "0.3.0",
    "ru-validation-codes": "2.8.0",
    "rxjs": "6.5.3",
    "tslib": "2.0.0",
    "uuid": "8.3.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.3",
    "@angular-devkit/build-ng-packagr": "0.1000.5",
    "@angular-eslint/builder": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
    "@angular-eslint/schematics": "0.0.1-alpha.32",
    "@angular-eslint/template-parser": "0.0.1-alpha.32",
    "@angular/cli": "10.1.0",
    "@angular/compiler-cli": "10.1.1",
    "@types/jest": "26.0.10",
    "@types/node": "12.11.1",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "codelyzer": "6.0.0",
    "cpx": "^1.5.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "9.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jest": "^26.4.2",
    "jest-junit": "11.1.0",
    "jest-preset-angular": "^8.3.1",
    "json-server": "^0.16.2",
    "lint-staged": "10.2.11",
    "ng-bullet": "1.0.3",
    "ng-mocks": "10.1.2",
    "ng-packagr": "10.0.0",
    "prettier": "2.0.5",
    "protractor": "7.0.0",
    "standard-version": "^9.0.0",
    "stylelint": "13.6.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "ts-jest": "26.2.0",
    "ts-node": "8.3.0",
    "typescript": "3.9.5"
  }
}
