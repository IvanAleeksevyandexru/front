{
  "name": "epgu-form-frontend",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main",
    "ng": "ng",
    "start": "ng serve --proxy-config ./src/proxy.conf.json",
    "start:stage": "ng serve --configuration staging",
    "start:backend": "java -jar fs-1.0-SNAPSHOT.jar --server.servlet.context-path=/api",
    "build": "ng build",
    "library:build": "ng build epgu-constructor",
    "library:watch": "ng build epgu-constructor --watch",
    "library:test": "ng test epgu-constructor",
    "library:lint": "ng lint epgu-constructor",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test:dev": "jest --watch -o",
    "test:ci": "jest --ci",
    "test:coverage": "jest --ci --coverage --no-cache --reporters=default --reporters=jest-junit --coverageDirectory=coverage/jest",
    "e2e": "ng e2e"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "jest-junit": {
    "suiteName": "epgu-form-frontend",
    "outputDirectory": "./coverage/junit",
    "outputName": "test-results.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "dependencies": {
    "@angular/animations": "~10.0.4",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/forms": "^10.0.10",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/router": "~10.0.4",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "angular2-text-mask": "9.0.0",
    "epgu-lib": "^0.0.384",
    "moment": "2.24.0",
    "ngx-perfect-scrollbar": "8.0.0",
    "ru-validation-codes": "^2.8.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular-devkit/build-ng-packagr": "~0.1000.5",
    "@angular-eslint/builder": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
    "@angular-eslint/schematics": "0.0.1-alpha.32",
    "@angular-eslint/template-parser": "0.0.1-alpha.32",
    "@angular/cli": "~10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@types/jest": "^26.0.10",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "codelyzer": "^6.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "jest-junit": "^11.1.0",
    "jest-preset-angular": "^8.2.1",
    "lint-staged": "^10.2.11",
    "ng-bullet": "^1.0.3",
    "ng-mocks": "^10.1.2",
    "ng-packagr": "^10.0.0",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "ts-jest": "^26.2.0",
    "ts-node": "~8.3.0",
    "typescript": "~3.9.5"
  }
}
