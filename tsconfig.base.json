{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@epgu/children-clubs": [
        "dist/children-clubs"
      ],
      "@epgu/epgu-constructor-ui-kit": [
        "dist/epgu-constructor-ui-kit"
      ]
    }
  }
}
